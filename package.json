  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --env.env=prod --config config/webpack.config.js",
    "dev": "webpack-dev-server --config config/webpack.config.js --mode development --open",
    "start": "webpack-dev-server --env.env=dev --config config/webpack.config.js",
    "prod": "npm run clean && webpack --env.env=prod --config config/webpack.config.js",
    "dev:addon": "webpack-dev-server --env.env=dev --env.addons=myplugin --config config/webpack.config.js  --open",
    "dev:analyze": "webpack-dev-server --env.env=dev --env.addons=analyze --config config/webpack.config.js",
    "prod:analyze": "webpack --env.env=prod --env.addons=analyze --config config/webpack.config.js",
    "test": "jest"
  },
